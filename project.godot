;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="PaddleBattle"
config/description="A multiplayer action game with pong-like mechanics"
run/main_scene="res://Scenes/Main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="PaddleBattle"
boot_splash/image="res://Assets/Icon.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.101961, 0.101961, 0.101961, 1 )
config/icon="res://Assets/Icon.png"

[audio]

default_bus_layout=""

[autoload]

Game="*res://Scripts/Autoloads/Game.cs"
DiscordManager="*res://Scripts/Autoloads/DiscordManager.cs"
InputManager="*res://Scripts/Autoloads/InputManager.cs"

[debug]

settings/fps/force_fps=120
settings/stdout/verbose_stdout=true
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1280
window/size/height=720
window/dpi/allow_hidpi=true
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="expand"

